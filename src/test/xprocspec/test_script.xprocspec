            <head>
	            <meta name="dc:Title" content="title"/>
	            <meta name="dc:Creator" content="celia"/>
	          </head>
            <book>
              <frontmatter>
                <doctitle>title</doctitle>
                <docauthor>celia</docauthor>
              </frontmatter>
              <bodymatter>
                <level1>
                  <h1>h1 a</h1>
		  <p>level1</p>
		  <level2>
		    <h2>h2 a</h2>
		    <p>level2</p>
		    <level3>
		      <h3>h3 a</h3>
		      <p>level3</p>
		    </level3>
		  </level2>
                </level1>
