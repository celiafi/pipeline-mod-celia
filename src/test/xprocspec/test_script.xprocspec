		  <h1>h1 b</h1>
		  <p>level1</p>
		</level1>
              </bodymatter>
            </book>
          </dtbook>
        </x:document>
      </x:input>
      <x:option name="include-brf" select="true()"/>
      <x:option name="pef-output-dir" select="concat($temp-dir, 'table-of-contents/output-dir/')"/>
      <x:option name="brf-output-dir" select="concat($temp-dir, 'table-of-contents/output-dir/')"/>
      <x:option name="temp-dir" select="concat($temp-dir, 'table-of-contents/temp-dir/')"/>
